{
  "NewtonsMethod": {
    "params": {
      "image_specification": {
        "resolution": [1920, 1080],
        "center": [0, 0],
        "width": 5.0
      },
      "max_iteration_count": 500,
      "convergence_tolerance": 1e-8,
      "render_options": {
        "downsample_stride": 1,
        "subpixel_antialiasing": 4
      },
      "boundary_set_color_rgb": [0, 0, 0],
      "cyclic_attractor_color_rgb": [255, 255, 255],
      "root_colors_rgb": [
        [18, 83, 53],
        [170, 30, 78],
        [30, 95, 175],
        [228, 173, 55]
      ],
      "grayscale_keyframes": [
        { "query": 0.0, "value": 0.5 },
        { "query": 0.92, "value": 0.75 },
        { "query": 1.0, "value": 1.0 }
      ],
      "lookup_table_count": 512,
      "histogram_bin_count": 512,
      "histogram_sample_count": 30000
    },
    "system": {
      "RootsOfUnity": {
        "n_roots": 4,
        "newton_step_size": 1.0
      }
    }
  }
}
